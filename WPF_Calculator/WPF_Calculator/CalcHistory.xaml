<Window x:Class="WPF_Calculator.CalcHistory"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF_Calculator"
        mc:Ignorable="d"
        Title="CalcHistory" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>            
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="200"></ColumnDefinition>
            <ColumnDefinition Width="*"></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <TextBlock Grid.Column="0" Text="Historie" FontSize="20" FontWeight="Bold" Margin="20,20,0,0"></TextBlock>
        <Button Content="Historie ophalen" Grid.Column="0" Height="30" Width="100" Margin="20,50,80,324"></Button>
        <Button Content="Cache legen" Grid.Column="0" Height="30" Width="100" Margin="20,115,80,289"></Button>
        <DataGrid Grid.Column="1" Grid.Row="0" Margin="20,20,20,20"></DataGrid>
    </Grid>
</Window>
